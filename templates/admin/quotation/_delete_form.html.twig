<form method="post" action="{{ path('admin_quotation_delete', {'id': quotation.id}) }}" onsubmit="return confirm('{% trans %}Are you sure you want to delete this item?{% endtrans %}');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ quotation.id) }}">
    <button class="btn">{% trans %}Delete{% endtrans %}</button>
</form>
